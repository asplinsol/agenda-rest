{
  "name": "agenda-rest",
  "version": "1.3.1",
  "description": "Scheduling as a Service",
  "main": "./dist/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "snyk-protect": "snyk protect",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.js --write $PWD'/**/*.js'",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "test": "ava ./dist/test.js && npm run format",
    "start": "npm run dev && node cli.js",
    "prepublish": "npm run snyk-protect && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/agenda/agenda-rest.git"
  },
  "files": [
    "cli.js",
    "settings.js",
    "dist"
  ],
  "keywords": [
    "schedule",
    "scheduling",
    "scheduler",
    "webservice",
    "microservice",
    "restful",
    "rest",
    "agenda"
  ],
  "author": "Keyvan Mir Mohammad Sadeghi <keyvan.m.sadeghi@gmail.com>",
  "license": "MIT",
  "bin": {
    "agenda-rest": "cli.js"
  },
  "bugs": {
    "url": "https://github.com/agenda/agenda-rest/issues"
  },
  "homepage": "https://github.com/agenda/agenda-rest#README",
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@babel/register": "7.12.1",
    "ava": "=2.4.0",
    "babel-loader": "8.2.2",
    "eslint": "6.8.0",
    "prettier": "2.1.2",
    "prettier-eslint": "11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "supertest": "4.0.2",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {
    "agenda": "^3.0.0",
    "async-counter": "^1.1.0",
    "babel-runtime": "^6.26.0",
    "commander": "^5.0.0",
    "koa": "^2.10.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^9.0.0",
    "pythonic": "^2.0.3",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "snyk": "^1.424.2"
  },
  "engines": {
    "node": ">=8"
  },
  "xo": {
    "space": 2,
    "rules": {
      "ava/no-inline-assertions": 0,
      "import/namespace": 0,
      "promise/param-names": 0,
      "promise/prefer-await-to-then": 0,
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    }
  },
  "snyk": true
}
